<!--- edit item popup (eipp) --->
<div class="popup pp-std" id="eipp">

    <div class="pp-content w-400 top-90" id="eippContent">

        <div class="pp-close pad-6" id="eippClose">
            <div class="pp-close-icon w-20 h-20">x</div>
        </div>

        <div class="ta-c fw-b fs-15" id="eippTitle"></div>

        <div class="flex-c jc-c ai-c rg-30 mt-30">
            <div class="div-input">
                <label class="label-std fs-13 l-3p">FOB</label>
                <input type="text" class="input-std ta-c w-160 pad-10-0-6-0" id="eippFob">
                <div class="mt-5 fs-12" id="eippFobText"></div>
            </div>
            <div class="div-input">
                <label class="label-std fs-13 l-3p">Cantidad (UM)</label>
                <input type="text" class="input-std ta-c w-160 pad-10-0-6-0" id="eippQty">
            </div>
        </div>

        <div class="fw-b fc-error fs-13 ta-c mt-10 not-visible" id="eippError">Debe completar todos los campos</div>

        <div class="flex-r cg-20 jc-c mb-13 mt-35">
            <div id="eippAccept" class="btn-std w-100 pad-6-0 fs-12">ACEPTAR</div>
            <div id="eippCancel" class="btn-std w-100 pad-6-0 fs-12">CANCELAR</div>
        </div>

    </div>
</div>

<!----------LOADER---------->
<div class="loader ld-std" id="eippLoader">
    <div class="ld-content ld-main top-45p"></div>
</div>